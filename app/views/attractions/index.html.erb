<h1>List of Attractions</h1> <br>

<ul>
    <% @attractions.each do |att| %>
        <li> <%= link_to "Go on #{att.name}", attraction_path(att) if !@user.admin%> </li>
        <li> <%= link_to "Show #{att.name}", attraction_path(att) if @user.admin%> </li>
    <% end %>
</ul> <br>

<li> <%= link_to "New Attraction", new_attraction_path if @user.admin%> </li>
